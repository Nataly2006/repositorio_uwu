/* function es printSumNums */
function sumNums(num1, num2) {
    var sum = num1 + num2;
    console.log('sum is', sum);
    return sum;
}
var result = sumNums(2,3) + sumNums(3,5);
console.log('result is', result);
/*function= sumNums (num1, num2)
DIAGRAMA T
variable= sum  result
values= num1 + num2   sumNums(2,3) + sumNums(3,5); */

function sumNums(num1, num2) {
    var sum = num1 + num2;
    console.log('sum is', sum);
    return sum;
}
var result = sumNums(2,3) + sumNums(3,sumNums(2,1)) + sumNums(sumNums(2,1),sumNums(2,3));
console.log('result is', result);

/* function es sumNums(num1 ,num2)
DIAGRAMA T
variable = sum result 
values =  num1 + num2  sumNums(2,3) + sumNums(3,sumNums(2,1)) + sumNums(sumNums(2,1),sumNums(2,3))
*/
