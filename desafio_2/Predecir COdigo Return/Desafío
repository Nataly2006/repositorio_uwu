        console.log('hello');  imprime hello
        console.log('Dojo');  Imprime Dojo


*/
function hello() {
    console.log('hello');
    return 15;
}
var result = hello();
console.log('result is', result);
// console.log('result is', result);  imprimimos hello is ,hello //

/* funcion= hello
DIAFRAMA T
variable =result
values = hello();
*/
function numPlus(num) {
    console.log('num is', num);
    return num+15;
}
var result = numPlus(3);
console.log('result is', result);
/* DIAFRAMA T
variable = result 
values = numPlus(3);
*/
var num = 15;
console.log(num);
function logAndReturn(num2){
console.log(num2);   
return num2;
}
var result = logAndReturn(10);
console.log(result);
console.log(num);

/* function =logAndreturn (10)
DIAGRAMA T
variable = num result
values = 15  logAndReturn 
*/
var num = 15;
console.log(num);
function timesTwo(num2){
console.log(num2);   
return num2*2;
}
var result = timesTwo(10);
console.log(result);
console.log(num);

/* la function es timesTWo(10)
DIAGRAMA T
variable = num  result
values = 15   timesTWo
*/
function timesTwoAgain(num) {
    console.log('num is', num);
    var y = num*2;
    return y;
}
var result = timesTwoAgain(3) + timesTwoAgain(5);
console.log('result is', result);

/* function = timesTWoAgain(num)
DIAGRAMA T
Variables= y result
values= num*2  timesTwoAgain(3) + timesTwoAgain(5) 
*/
